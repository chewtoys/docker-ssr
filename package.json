{
  "name": "docker-ssr",
  "version": "0.0.1",
  "description": "A Dockerized SSR runtime to host your frontend webapp with",
  "main": "server.js",
  "repository": "git@github.com:thegreenhouseio/docker-ssr.git",
  "author": "Owen Buckley <owenbuckley13@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint *.js ./src/**/*.jsx",
    "develop": "webpack-dev-server --config ./webpack.config.develop.js",
    "build": "rimraf ./static && webpack --config ./webpack.config.common.js",
    "serve": "yarn run build && babel-node server.js",
    "start": "babel-node server.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
